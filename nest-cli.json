{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/fin-freelancer/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/fin-freelancer/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/fin-freelancer",
  "watchOptions": {
    "poll": true
  },
  "projects": {
    "billing": {
      "type": "application",
      "root": "apps/billing",
      "entryFile": "main",
      "sourceRoot": "apps/billing/src",
      "compilerOptions": {
        "tsConfigPath": "apps/billing/tsconfig.app.json"
      }
    },
    "clients": {
      "type": "application",
      "root": "apps/clients",
      "entryFile": "main",
      "sourceRoot": "apps/clients/src",
      "compilerOptions": {
        "tsConfigPath": "apps/clients/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "fin-freelancer": {
      "type": "application",
      "root": "apps/fin-freelancer",
      "entryFile": "main",
      "sourceRoot": "apps/fin-freelancer/src",
      "compilerOptions": {
        "tsConfigPath": "apps/fin-freelancer/tsconfig.app.json"
      }
    },
    "identity": {
      "type": "application",
      "root": "apps/identity",
      "entryFile": "main",
      "sourceRoot": "apps/identity/src",
      "compilerOptions": {
        "tsConfigPath": "apps/identity/tsconfig.app.json"
      }
    }
  }
}